{
  "extends": ["airbnb", "airbnb/hooks", "prettier", "prettier/react", "plugin:prettier/recommended"],
  "plugins": ["prettier"],

  "env": {
    "browser": true,
    "es2020": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "warn",
    "func-names": "off",
    "no-eval": "error",
    "import/first": "error",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "root": true
}
